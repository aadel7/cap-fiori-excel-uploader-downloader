{
  "name": "ExcelUploaderDownloader",
  "version": "1.0.0",
  "description": "Excel Uploader Downloader + HANA Cloud + Fiori app",
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/http-client": "^4.4.0",
    "@sap-cloud-sdk/util": "^4.4.0",
    "@sap/cds": "^9",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.12.2",
    "dotenv": "^17.2.3",
    "express": "^4",
    "hdb": "^2.26.4",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9",
    "@sap/dev-cap-tools": "^1.31.0",
    "cds-plugin-ui5": "^0.13.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-exceluplaoderdownloader-app": "cds watch --open exceluplaoderdownloaderapp/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy ExcelUploaderDownloader --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "private": true,
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "lcap": true,
  "workspaces": [
    "app/*"
  ]
}
